    <application
        android:name="App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <!-- 新浪微博分享回调会用到 -->
            <intent-filter>
                <action android:name="com.sina.weibo.sdk.action.ACTION_SDK_REQ_ACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!-- 这个为什么不放在 shareLib合并的时候回有问题, 先后顺序不能变 -->
        <activity
            android:name="www.leg.com.sharelib.BaseWXEntryActivity"
            android:exported="true" />

        <activity-alias
            android:name="org.uuyi.youyi.wxapi.WXEntryActivity"
            android:exported="true"
            android:targetActivity="www.leg.com.sharelib.BaseWXEntryActivity" />

        <activity-alias
            android:name="org.uuyi.youyi.wxapi.WXPayEntryActivity"
            android:exported="true"
            android:targetActivity="www.leg.com.sharelib.BaseWXEntryActivity" />

    </application>


